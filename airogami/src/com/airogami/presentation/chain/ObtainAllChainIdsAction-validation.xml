<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
		"http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>
	<field name="startId">
     	<field-validator type="required">
			<message key="error.chain.obtainchainids.startId.required">Please input a startId</message>
		</field-validator>
	</field>

	<field name="limit">
     	<field-validator type="required">
			<message key="error.chain.obtainchainids.limit.required">Please input a limit</message>
		</field-validator>
	</field>
	
</validators>