<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
		"http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>
	<field name="planeId">
     	<field-validator type="fieldexpression">
     	<param name="expression"><![CDATA[!(planeId == null && method == "throwPlane")]]></param>
			<message key="error.plane.likedeleteplane.planeId.required">Please input a planeId</message>
		</field-validator>
		<field-validator type="long">
		    <param name="min">1</param>
			<message key="error.plane.likedeleteplane.planeId.invalid">The planeId must be over ${min}</message>
		</field-validator>
	</field>

</validators>