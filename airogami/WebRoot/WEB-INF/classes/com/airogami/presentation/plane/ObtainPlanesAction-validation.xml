<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
		"http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>
	<field name="startIdx">
     	<field-validator type="required">
			<message key="error.plane.obtainplanes.startIdx.required">Please input a startIdx</message>
		</field-validator>
	</field>
	
	<field name="start">
		<field-validator type="regex">
			<param name="expression"><![CDATA[\d{4}-\d{2}-\d{2} \d{2}:\d{2}:\d{2}(.\d{1,3})?]]></param>
			<message key="error.plane.obtainplanes.start.invalid">The start seems invalid</message>
		</field-validator>
	</field>
	
	<field name="end">
		<field-validator type="regex">
			<param name="expression"><![CDATA[\d{4}-\d{2}-\d{2} \d{2}:\d{2}:\d{2}(.\d{1,3})?]]></param>
			<message key="error.plane.obtainplanes.end.invalid">The end seems invalid</message>
		</field-validator>
	</field>

	<field name="limit">
     	<field-validator type="required">
			<message key="error.plane.obtainplanes.limit.required">Please input a limit</message>
		</field-validator>
	</field>
	
	<field name="forward">
     	<field-validator type="required">
			<message key="error.plane.obtainplanes.forward.required">Please input a forward</message>
		</field-validator>
	</field>
	
</validators>