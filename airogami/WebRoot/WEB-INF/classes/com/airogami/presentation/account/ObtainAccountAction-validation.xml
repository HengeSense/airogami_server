<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
		"http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>

    <field name="accountId">
		<field-validator type="required">
			<message key="error.account.obtainaccount.accountId.required">Please input an accountId</message>
		</field-validator>
		<field-validator type="long">
			<param name="min"><![CDATA[1]]></param>
			<message key="error.account.obtainaccount.accountId.invalid">The accountId should be greater than ${min}</message>
		</field-validator>

	</field>

	<field name="last">
		<field-validator type="requiredstring">
     	   	<message key="error.account.obtainaccount.last.required">Please input a last</message>
		</field-validator>
		<field-validator type="regex">
			<param name="expression"><![CDATA[\d{4}-\d{2}-\d{2} \d{2}:\d{2}:\d{2}(.\d{1,3})?]]></param>
			<message key="error.account.obtainaccount.last.invalid">The last seems invalid</message>
		</field-validator>
	</field>
	
</validators>